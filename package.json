{
  "name": "node-auth3-project",
  "version": "1.1.0",
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon",
    "debug": "nodemon --inspect ./bin/www",
    "dev": "concurrently \"npm run debug\" \"npm run client\"",
    "frontDev": "concurrently \"npm run start\" \"npm run client\"",
    "client": "npm start --prefix client"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "connect-session-knex": "^1.5.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "http-errors": "~1.6.3",
    "knex": "^0.20.8",
    "knex-cleaner": "^1.3.0",
    "morgan": "~1.9.1",
    "prettier": "^1.19.1",
    "sequelize": "^5.21.3",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^2.0.2"
  }
